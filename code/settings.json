{
    // ----------- vim -------------
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": "\\",
    "vim.handleKeys": {},
    // ------- vim plugins ---------
    "vim.sneak": true,
    "vim.surround": true,
    "vim.camelCaseMotion.enable": true,
    // -------- vim keymap ----------
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            // TOOD maybe update 
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ],
            "silent": true
        },
        {
            "before": [
                "[",
                "b",
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "]",
                "b",
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "whichkey.show"
            ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "whichkey.show"
            ]
        }
    ],
    //------- vim  air-line --------
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [
        "#8FBCBB",
        "#434C5E"
    ],
    "vim.statusBarColors.insert": "#BF616A",
    "vim.statusBarColors.visual": "#B48EAD",
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#ffffff",
    "vim.statusBarColors.searchinprogressmode": "#ffffff",
    "vim.statusBarColors.easymotionmode": "#ffffff",
    "vim.statusBarColors.easymotioninputmode": "#ffffff",
    "vim.statusBarColors.surroundinputmode": "#ffffff",
